{
	"stack": "nodejs",
	"memory": 1024,
	"runtimeArgs": ["--max_old_space_size=800"],
	"ttl": 300,
	"timeout": 1000,
	"routes": {
		"tasks": {
			"path": "/_v/collections/tasks",
			"public": true
		},
		"collections": {
			"path": "/_v/collections",
			"public": true
		},
    "collectionsDetail": {
			"path": "/_v/collections/:id",
			"public": true
		},
    "deleteCollection": {
      "path": "/_v/collections/delete/:id",
      "public": true
    },
    "cloneCollection": {
      "path": "/_v/collections/clone/:id",
      "public": true
    }
	}
}
